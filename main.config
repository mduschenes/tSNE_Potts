
### Config File Template ###


[GENERATE]
NAMEFORMAT = job_%d
ITERABLES = model
DIRECTORY= Jobs
SUBMIT = latticemodel.py

[model]
L = 15
d = 2
q = 10
T = 0.75
name = potts
couplings = (0,1)
dtype = int

[simulation]
Neqb = 100
Nmeas = 1000
Nmeas_freq = 10
Nmeas_ratio = 0.7
algorithm = metropolis_wolff

[analysis]
simulation = True
analysis = True
sorting = True
plotting = False
quiet = False
observables = energy,order,specific_heat,susceptibility
sort_params = L,q,T

[job]
filetype = json




