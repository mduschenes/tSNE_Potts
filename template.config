
### Config File Template ###

[CONFIG]
FILE = job_%d
ARGUMENTS = model
PARAMETERS = model,simulation
FILETYPE = json
OBSERVABLES = energy,order,specific_heat,susceptibility

[model]
L = 6
d = 2
q = 4
T = 0.6
name = potts
couplings = (0,1)
dtype = int

[simulation]
Neqb = 10
Nmeas = 10
Nfreq = 1
Nratio = 0.5
algorithm = metropolis_wolff
filetype = json
plotting = True
quiet = False






